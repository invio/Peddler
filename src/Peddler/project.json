{
  "name": "Peddler",
  "title": "Data Generation for .NET",
  "version": "0.2.0",
  "authors": [ "Brian Caruso <brian@invioinc.com>" ],

  "packOptions": {
    "summary": "Data Generation for .NET",
    "tags": [ "dependencyinjection", "di" ],
    "owners": [ "Invio, Incorporated" ],
    "licenseUrl": "https://github.com/invio/Peddler/blob/master/LICENSE.txt",
    "projectUrl": "https://github.com/invio/Peddler/",
    "requireLicenseAcceptance": false,
    "repository": {
